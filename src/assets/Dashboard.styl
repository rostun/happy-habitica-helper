.Dashboard
   display flex
   flex-direction column
   align-items center
   h1
      color red

   .search_results
      .pet_information
         display grid
         grid-template-columns repeat(8, 1fr)
         grid-gap 10px
         grid-auto-rows 1fr
         list-style-type none
         padding 0
         li
            border 1px solid
            padding 8px
            border-radius 5px
